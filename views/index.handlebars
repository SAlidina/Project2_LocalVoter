<h1>Bills</h1>


  {{#each billsData}}
  <form style = "border: 2px solid"> 
  <div class="form-group">
    <label for="BillIDBillName"><b>{{StateBillID}}</b> : {{BillName}}</label>
  </div>
  <div class="form-group">
    <label for="ActionDate"> {{ActionDate}}</label>
  </div>
    <div class="form-group">
    <label for="Create"> {{Create}}</label>
  </div>
    <div class="form-group">
    <label for="BillProgress"> {{BillProgress}}</label>
  </div>
  <div class="form-group">
     <label for="Sponsors"><b>Sponsors</b> : {{Sponsors}}</label>
  </div>
  <div class="form-group">
    <label for="Summary"><b>Summary</b>:</label>
    <textarea class="form-control" id="Summary" rows="3">{{Summary}}</textarea>
  </div>
  <div class="form-group">
    <label for="OfficialDocument"><b>More Info</b>: <a href={{OfficialDocument}}>{{OfficialDocument}}</a></label>
  </div>
</form>
<br>
  {{/each}}


