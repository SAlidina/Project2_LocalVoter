

{{!-- login --}}
<div class="row">
  <div class="container">
    <div class="row">
      <div class="col s12 m6 offset-m3">
        <div class="card">

          <div class="center">
            <img src="assets/PersonIcon_Blue.png" alt="Logo" class="responsive-img valign profile-image-login"
              style="width:100px; height:100px; margin-top: 15px">
          </div>
          <div class="card-content">

            <div class="form-field">
              <label for="usernameLI">Username</label>
              <input type="text" id="usernameLI" class="validate">
            </div><br>

            <div class="form-field">
              <label for="passwordLI">Password</label>
              <input type="text" id="passwordLI" class="validate">
            </div><br>

            <div class="form-field">
              <label>
                <input id="indeterminate-checkbox" type="checkbox">
                <span>Remember Me!</span>
              </label>
            </div><br>

            <div class="form-field">
              <a href="/bills/all"><button class="btn-large indigo darken-4 waves-effect waves-light btn" id="loginUser"
                  style="width:100%">Login</button></a>
            </div><br>

            <div class="links">
              <p class="center-align"><a href="">I forgot my password</a></p>
              <p class="center-align">New?<a href="/signup"> Create an account</a></p>
            </div>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>

