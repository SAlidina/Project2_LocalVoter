<div class="row">
  <div class="col s10 offset-s1">
    <div class="card blue-grey">
      <div class="card-content white-text">
        <span class="card-title">{{StateBillID}}: {{BillName}}</span>
        <p>{{Summary}}</p>
      </div>
      <div class="card-action">
        <a id="billTrack" href="/bills/bill={{BillID}}" data-billID="{{BillID}}">Track this bill..</a>
        <a id ="learnMore" href="/bills/bill={{BillID}}">Learn more..</a>
      </div>
    </div>
  </div>
</div>
